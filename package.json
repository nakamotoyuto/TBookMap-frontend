{
  "name": "tbookmap-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "fix:lint": "eslint . --ext ts,tsx,js,jsx",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx}\" && npm run fix:lint -- --fix",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@babel/core": "7.17.10",
    "@storybook/addon-postcss": "2.0.0",
    "@tailwindcss/line-clamp": "0.4.0",
    "axios": "0.27.2",
    "framer-motion": "6.3.3",
    "js-cookie": "3.0.1",
    "next": "12.1.5",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-modal": "3.15.1",
    "recoil": "0.7.2",
    "swr": "1.3.0"
  },
  "devDependencies": {
    "@emotion/babel-preset-css-prop": "11.2.0",
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@storybook/addon-actions": "6.4.22",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/addon-interactions": "6.4.22",
    "@storybook/addon-links": "6.4.22",
    "@storybook/builder-webpack5": "6.4.22",
    "@storybook/manager-webpack5": "6.4.22",
    "@storybook/react": "6.4.22",
    "@storybook/testing-library": "0.0.11",
    "@types/js-cookie": "3.0.2",
    "@types/react": "18.0.9",
    "@types/react-modal": "3.13.1",
    "autoprefixer": "10.4.7",
    "babel-loader": "8.2.5",
    "eslint-config-prettier": "8.5.0",
    "eslint": "8.15.0",
    "eslint-config-next": "14.2.16",
    "postcss": "8.4.13",
    "prettier": "2.6.2",
    "react-hook-form": "7.30.0",
    "react-icons": "4.3.1",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.4",
    "webpack": "5.72.0"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 100
  }
}
